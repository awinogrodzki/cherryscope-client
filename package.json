{
  "name": "cherryscope-client",
  "version": "0.8.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=production webpack --bail --config webpack.config.js -p",
    "start": "webpack --config webpack.config.js --watch",
    "start:dev": "webpack-dev-server",
    "lint": "eslint --ext .jsx --ext .js src",
    "lint:fix": "eslint --ext .jsx --ext .js --fix src",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch:coverage": "jest --watch --coverage",
    "clean": "rm -rf dist",
    "deploy": "npm run build && firebase deploy && npm run clean",
    "release": "standard-version"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "fbemitter": "^2.1.1",
    "gsap": "^1.20.2",
    "i18n-js": "^1.0.0",
    "languages": "^0.1.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.5",
    "react-onclickoutside": "^6.1.1",
    "react-redux": "^5.0.4",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-transition-group": "^1.2.0",
    "react-youtube": "^7.4.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.0",
    "standard-version": "^4.2.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^3.0.1",
    "redux-mock-store": "^1.2.3",
    "style-loader": "^0.17.0",
    "svg-react-loader": "^0.4.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "engines": {
    "node": ">=7.1.0",
    "npm": ">=3.10.9"
  },
  "jest": {
    "modulePaths": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.js",
      "\\.(s?css)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
